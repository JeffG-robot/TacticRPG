import java.awt.image.*;
public abstract class MapObject {
  
private int x;
private int y;
BufferedImage image;
public void setImage(BufferedImage image1){
  this.image=image;
}
public BufferedImage getImage(){
  return image;
}
public void setX(int x){
  this.x=x;
}
public void setY(int y){
  this.y=y;
}
  public int getX(){
  return x;
  }
  public int getY(){
  return y;
  }
}
